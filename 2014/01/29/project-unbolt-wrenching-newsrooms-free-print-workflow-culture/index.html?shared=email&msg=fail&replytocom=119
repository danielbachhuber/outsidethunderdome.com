			<div class="reply">
				<a class='comment-reply-link' href='index.html?shared=email&amp;msg=fail&amp;replytocom=118#respond' onclick='return addComment.moveForm("comment-118", "118", "respond", "6496")'>Reply</a> <span>&darr;</span>			</div><!-- .reply -->
