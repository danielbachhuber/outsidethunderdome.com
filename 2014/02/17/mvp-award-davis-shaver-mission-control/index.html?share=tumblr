(function(root, exports, store) {
    'use strict';
    function decode(s) {
        var e={},i,b=0,c,x,l=0,a,r='',w=String.fromCharCode,L=s.length;
        var A='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/';
        for(i=0;i<64;i++){e[A.charAt(i)]=i;}
        for(x=0;x<L;x++){
            c=e[s.charAt(x)];b=(b<<6)+c;l+=6;
            while(l>=8){((a=(b>>>(l-=8))&0xff)||(x<(L-2)))&&(r+=w(a));}
        }
        return r;
    }
    function isFunction(fn) { return (typeof fn === 'function'); }
    function maybeCall(fn, val) { return isFunction(fn) ? fn.call(this, val) : fn; }
    try { store = JSON.parse((isFunction(root.atob)?root.atob:decode)(store)); } catch (err) { store = {}; }

    exports.Flags = function Flags(name) {
        var has = !!(store[name]);
        return function flag(accept, reject) {
            var result = maybeCall.call(this, (has ? accept : reject), has);
            return (result !== void 0) ? result : flag;
        };
    };

    exports.Flags.bool = function FlagsBool(name) {
        return !!(store[name]);
    };
